{
    "latex-workshop.latex.outDir": "%DIR%/outputs",
    "latex-workshop.latex.recipe.default": "Build German",
    "latex-workshop.latex.clean.fileTypes": [
        "%DOCFILE%.aux",
        "%DOCFILE%.bbl",
        "%DOCFILE%.blg",
        "%DOCFILE%.idx",
        "%DOCFILE%.ind",
        "%DOCFILE%.lof",
        "%DOCFILE%.lot",
        "%DOCFILE%.out",
        "%DOCFILE%.toc",
        "%DOCFILE%.acn",
        "%DOCFILE%.acr",
        "%DOCFILE%.alg",
        "%DOCFILE%.glg",
        "%DOCFILE%.glo",
        "%DOCFILE%.gls",
        "%DOCFILE%.fls",
        "%DOCFILE%.log",
        "%DOCFILE%.fdb_latexmk",
        "%DOCFILE%.snm",
        "%DOCFILE%.synctex(busy)",
        "%DOCFILE%.synctex.gz(busy)",
        "%DOCFILE%.nav",
        "%DOCFILE%.vrb"
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "Build German",
            "tools": [
                "pdflatex-ngerman"
            ]
        },
        {
            "name": "Build English",
            "tools": [
                "pdflatex-english"
            ]
        },
        {
            "name": "Build Dutch",
            "tools": [
                "pdflatex-dutch"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex-ngerman",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-output-directory=outputs",
                "\\PassOptionsToPackage{main=ngerman}{babel} \\input{%DOCFILE%}"
            ]
        },
        {
            "name": "pdflatex-english",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-output-directory=outputs",
                "\\PassOptionsToPackage{main=english}{babel} \\input{%DOCFILE%}"
            ]
        },
        {
            "name": "pdflatex-dutch",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-output-directory=outputs",
                "\\PassOptionsToPackage{main=dutch}{babel} \\input{%DOCFILE%}"
            ]
        }
    ]
}